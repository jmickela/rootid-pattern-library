<section class="container-fluid">
  <div class="row">
    {% for card in cards_threeacross %}
      <div class="col-md-4 col-sm-12">
        {% include "10-molecules/50-cards/10-card1/00-card1.twig" with {
          card_modifiers: ['align-center'],
          card_heading_level: 2,
          card_subtitle: card.subtitle,
          card_content: card.content,
          card_media: card.image,
          card_labels: card.labels,

          card_href: card.link,
          button_content: "Read More"|trans,
        } %}
      </div>
    {% endfor %}
  </div>
</section>