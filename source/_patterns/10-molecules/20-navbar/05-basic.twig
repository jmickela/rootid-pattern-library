  {% set mods = '' %}
  {% for mod in menu_modifiers %}
    {% set mods = mods ~ " navbar--" ~ mod %}
  {% endfor %}

<nav class='nabar navbar--basic {{ mods }}' role='navigation'>
  <ul class='nav navbar-nav'>
    {% for item in menu_items %}
      <li class='menu-item'>
        <a href='{{item.link}}'>{{item.title}}</a>
      </li>
    {% endfor %}
    {% block menu_extra_content %}{% endblock %}
  </ul>
</nav>