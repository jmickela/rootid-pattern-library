<section class="container-fluid">
  <div class="row">
    <div class="col-sm-12">
      <h2>Featured Content!</h2>
    </div>
  </div>
  <div class="row">
    {% for card in cards_threeacross %}
      <div class="col-md-4 col-sm-12">
        {% include "20-molecules/50-cards/30-card3/00-card3.twig" with {
          card_heading_level: 3,
          card_subtitle: card.subtitle,
          card_content: card.content,
          card_media: card.image,
          card_labels: card.labels,

          card_href: card.link,
        } %}
      </div>
    {% endfor %}
  </div>
</section>