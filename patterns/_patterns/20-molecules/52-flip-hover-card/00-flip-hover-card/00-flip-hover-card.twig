{% if not_article %}
  <div class="fliphovercard {{ mods }}">
{% else %}
  <article class="fliphovercard {{ mods }}">
{% endif %}

  <div class="fliphovercard__inner">
    <div class="fliphovercard__front">
      {% include "50-cards/10-card1/00-card1.twig" with {
        not_article: true,
        card_modifiers: flipcard_front.card_modifiers,
        card_media: flipcard_front.card_media,
        card_img_src: flipcard_front.card_img_src,
        card_img_alt: flipcard_front.card_img_alt,
        card_img_lazy_load: flipcard_front.card_img_lazy_load,
        card_img_srcset: flipcard_front.card_img_srcset,
        card_img_sizes: flipcard_front.card_img_sizes,

        card_labels: flipcard_front.card_labels,

        card_subtitle: flipcard_front.card_subtitle,
        card_content: flipcard_front.card_content,
        card_href: flipcard_front.card_href,
        button_href: flipcard_front.button_href,
        button_content: flipcard_front.button_content

      } only %}
    </div>

    <div class="fliphovercard__back">
      {% include "50-cards/10-card1/00-card1.twig" with {
        not_article: true,
        card_modifiers: flipcard_back.card_modifiers,
        card_media: flipcard_back.card_media,
        card_img_src: flipcard_back.card_img_src,
        card_img_alt: flipcard_back.card_img_alt,
        card_img_lazy_load: flipcard_back.card_img_lazy_load,
        card_img_srcset: flipcard_back.card_img_srcset,
        card_img_sizes: flipcard_back.card_img_sizes,

        card_labels: flipcard_back.card_labels,

        card_subtitle: flipcard_back.card_subtitle,
        card_content: flipcard_back.card_content,
        card_href: flipcard_back.card_href,
        button_href: flipcard_back.button_href,
        button_content: flipcard_back.button_content
      } only %}
    </div>
  </div>


{% if not_article %}
  </div>
{% else %}
  </article>
{% endif %}