  <div class="listingpage__localmenu">
    {% include "20-molecules/20-navigation/40-inline-menu/10-local-menu.twig" with {
      menu_items: local_menu
    } only %}
  </div>

  <div class="listingpage__description">
    {{ listing_hero__description }}
  </div>
{% endblock %}

{% block content %}
  {% include "40-templates/20-listing-page/10-listing-page.twig" with {
    page_content: page_content,
  } only %}
{% endblock %}

{% block post_content %}
