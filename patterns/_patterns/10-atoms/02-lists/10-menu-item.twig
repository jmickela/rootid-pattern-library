{% for item in menu_items  %}
  <li {{ bem('menu-item', menu_modifier, 'navmenu') }}>
    <a href='{{item.link}}'>{{item.title}}</a>

    {% if item.children %}
      <ul {{ bem('dropdown-menu', null, 'navmenu') }}>
        {% include "10-atoms/02-lists/10-menu-item.twig" with {
          menu_items: item.children,
        } %}
      </ul>
    {% endif %}
  </li>
{% endfor %}